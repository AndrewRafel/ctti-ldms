{% extends 'registration/base.html.twig' %}
{% block body %}
    
<table class="table">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Role</th>
    </tr>
  </thead>
  {% for user in users %}
  <tbody>
    <tr>
      <td>{{user.email}}</td>
      <td>{{user.getFirstName}}</td>
      <td>{{user.getLastname}}</td>
    </tr>
  </tbody>
</table>
    {% else %}
        
    {% endfor %}
{% endblock %}